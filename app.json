{
    "expo": {
        "name": "LT-Store",
        "slug": "LT-Store",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true
        },
        "web": {
            "favicon": "./assets/images/favicon.png"
        },
        "android": {
            "package": "com.liquidthoughttest.app",
            "versionCode": 1,
            "permissions": [],
            "icon": "./assets/images/icon.png"
        }
    }
}